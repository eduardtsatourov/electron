{
  "name": "jworks-demo-app",
  "version": "0.0.0",
  "main": "main.js",
  "author": "Ordina",
  "description": "BaseApp With Electron",
  "homepage": "./",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "electron:build": "npm run build && electron-packager . --overwrite --prune --platform win32 --arch x64 --out ./out",
    "electron:builddev": "ng build --base-href ./ && electron-packager . --overwrite",
    "electron": "wait-on tcp:4200 && electron .",
    "package:win" : "electronwin.bat",
    "package:mac" : "electronmac.bat"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/cdk": "~9.1.1",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/material": "~9.1.1",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "electron-is-dev": "^1.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/language-service": "~9.0.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.16.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "wait-on": "^5.2.1"
  }
}
